<?xml version="1.0"?>

<PropertyList>

 <path>OilTemp.ac</path>

   <animation>
  <type>material</type>
  <object-name>PointerTip</object-name>
  <object-name>Face</object-name>
     <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
      <red>   0.8 </red>
      <green> 0.6 </green>
      <blue>  0.3 </blue>
    </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>PointerTip</object-name>
  <property>engines/engine/oil-temperature-degf</property>
  <interpolation>
   <entry><ind>0</ind><dep>-15</dep></entry>
   <entry><ind>100</ind><dep>0</dep></entry>
   <entry><ind>180</ind><dep>50</dep></entry>
   <entry><ind>210</ind><dep>100</dep></entry>
   <entry><ind>250</ind><dep>111</dep></entry>
   <entry><ind>400</ind><dep>140</dep></entry>
   <entry><ind>590</ind><dep>160</dep></entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
 </animation>

</PropertyList>
